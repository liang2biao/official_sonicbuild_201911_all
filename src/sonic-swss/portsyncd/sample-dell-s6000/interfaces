# interfaces file
#
# The main configuration file that is located in folder /etc/network/.

# Loopback network interface
auto lo
iface lo inet loopback
    up ip addr add dev lo 10.1.0.33/32

# Management network interface
auto eth0
iface eth0 inet static
    address 10.3.147.1
    netmask 255.255.254.0
    gateway 10.3.146.1

# Switch front panel interfaces
auto Ethernet8
iface Ethernet8 inet static
